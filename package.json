  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "sass --watch ./styles/scss/:./styles/unprefixed-css/ --style expanded --no-source-map",
    "prefix": "postcss ./styles/unprefixed-css/ --use autoprefixer -d ./styles/css/ --no-map ",
    "autoprefix": "onchange \"./styles/unprefixed-css/*.css\" -- npm run prefix",
